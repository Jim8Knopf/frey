[Unit]
Description=Frey WiFi Captive Portal Auto-login Daemon
After=network-online.target
Wants=network-online.target

[Service]
ExecStart=/usr/local/bin/frey-wifi-captive-portal-daemon.sh
Restart=always
RestartSec=30
User=root
StandardOutput=journal
StandardError=journal

[Install]
WantedBy=multi-user.target